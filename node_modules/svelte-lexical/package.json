{
  "name": "svelte-lexical",
  "description": "Rich Text editor for Svelte based on lexical",
  "version": "0.1.8",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/umaranis/svelte-lexical",
    "directory": "svelte-lexical"
  },
  "svelte": "src/index.ts",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "test": "jest src",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint .",
    "svelte-check": "svelte-check"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-jest": "^28.1.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "rollup": "^3.5.1",
    "rollup-plugin-svelte": "^7.1.0",
    "svelte-check": "^2.10.0",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "typescript": "^4.9.3"
  },
  "keywords": [
    "lexical",
    "editor",
    "rich-text",
    "list",
    "svelte"
  ],
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "@lexical/clipboard": "^0.7.6",
    "@lexical/code": "^0.7.6",
    "@lexical/file": "^0.7.6",
    "@lexical/hashtag": "^0.7.6",
    "@lexical/history": "^0.7.6",
    "@lexical/link": "^0.7.6",
    "@lexical/list": "^0.7.6",
    "@lexical/mark": "^0.7.6",
    "@lexical/overflow": "^0.7.6",
    "@lexical/plain-text": "^0.7.6",
    "@lexical/rich-text": "^0.7.6",
    "@lexical/selection": "^0.7.6",
    "@lexical/table": "^0.7.6",
    "@lexical/utils": "^0.7.6",
    "lexical": "^0.7.6",
    "svelte": "^3.53.1"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "svelte-jester"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
